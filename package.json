{
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "fix": "npm run -s fix:prettier && npm run -s fix:eslint",
    "fix:prettier": "prettier --write source",
    "fix:eslint": "eslint source --ext .ts --fix",
    "lint": "npm run -p lint:prettier && npm run -p lint:eslint",
    "lint:prettier": "prettier --check source",
    "lint:eslint": "eslint source --ext .ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/animejs": "^3.1.7",
    "@types/three": "^0.155.0",
    "animejs": "^3.2.1",
    "three": "^0.153.0"
  }
}
